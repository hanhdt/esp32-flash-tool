    "vue-router": "^3.4.9",
    "vuex": "^3.6.0",
    "vuex-electron": "^1.0.3",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "ajv": "^6.12.6",
    "babel-core": "^6.26.3",
